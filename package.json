{
    "name": "parcel-plugin-nunjucks",
    "version": "2.2.2",
    "description": "Parcel support for nunjucks templates",
    "author": "chocolateboy <chocolate@cpan.org>",
    "contributors": "Matthew McCune <git@mattrick.me>",
    "license": "MIT",
    "main": "dist/index.js",
    "module": "dist/index.esm.js",
    "repository": "https://github.com/chocolateboy/parcel-plugin-nunjucks",
    "scripts": {
        "build": "bili --map --format cjs src/*.js",
        "build:release": "cross-env NODE_ENV=production bili --no-map --format cjs,esm src/*.js",
        "clean": "shx rm -rf dist .*cache*/ test/tests/*/dist",
        "prepublishOnly": "run-s clean build:release test:run",
        "rebuild": "run-s clean build",
        "test": "run-s rebuild test:run",
        "test:run": "ava --verbose"
    },
    "files": [
        "dist/index.esm.js",
        "dist/index.js",
        "dist/NunjucksAsset.esm.js",
        "dist/NunjucksAsset.js"
    ],
    "ava": {
        "babel": true,
        "files": [
            "test/tests/*/test.js"
        ]
    },
    "browserslist": "maintained node versions",
    "dependencies": {
        "cosmiconfig": "^7.0.0",
        "nunjucks-parser": "^1.1.0"
    },
    "devDependencies": {
        "@ava/babel": "^1.0.1",
        "@babel/core": "^7.11.6",
        "ava": "^3.13.0",
        "babel-plugin-source-map-support": "^2.1.3",
        "bili": "^5.0.5",
        "core-js": "^3.6.5",
        "cross-env": "^7.0.2",
        "npm-run-all": "^4.1.5",
        "nunjucks": "^3.2.2",
        "parcel-bundler": "^1.12.4",
        "protochain": "^1.0.5",
        "shx": "^0.3.2",
        "source-map-support": "^0.5.19",
        "traverse": "^0.6.6"
    },
    "peerDependencies": {
        "nunjucks": "^3.0.0",
        "parcel-bundler": "^1.12.0"
    },
    "keywords": [
        "asset",
        "build",
        "nunjucks",
        "parcel",
        "parcel-plugin",
        "plugin",
        "template"
    ]
}
